{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "AdminKMSNoData",
        "Effect": "Allow",
        "Action": [
          "kms:Describe*",
          "kms:List*",
          "kms:Enable*",
          "kms:Disable*",
          "kms:Create*",
          "kms:Update*",
          "kms:Put*",
          "kms:TagResource",
          "kms:UntagResource",
          "kms:ScheduleKeyDeletion",
          "kms:CancelKeyDeletion"
        ],
        "Resource": "arn:aws:kms:eu-central-1:<ACCOUNT_ID>:key/<KEY_ID>"
      }
    ]
  }